{
	"name": "kindle-notes",
	"displayName": "KindleNotes",
	"description": "",
	"version": "0.0.1",
	"publisher": "thoqbk",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:kindle-notes.study",
		"onCommand:kindle-notes.studyThisFile",
		"onCommand:kindle-notes.syncBooks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"commandPalette": [
				{
					"command": "kindle-notes.studyThisFile",
					"when": "editorLangId == markdown"
				}
			]
		},
		"commands": [
			{
				"command": "kindle-notes.study",
				"title": "Kindle Notes: Study"
			},
			{
				"command": "kindle-notes.studyThisFile",
				"title": "Kindle Notes: Study This File"
			},
			{
				"command": "kindle-notes.syncBooks",
				"title": "Kindle Notes: Sync Books"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"@types/glob": "^7.2.0",
		"@types/js-yaml": "^4.0.5",
		"@types/lodash": "^4.14.180",
		"@types/md5": "^2.3.2",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"chai": "^4.3.6",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"dotenv": "^16.0.0",
		"front-matter": "^4.0.2",
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"md5": "^2.3.0",
		"puppeteer": "^13.4.1"
	}
}
